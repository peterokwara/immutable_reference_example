{
  "name": "immutable_reference_example",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "node ./dist/index",
    "build-clean": "rimraf ./dist/*",
    "build-compile": "tsc",
    "build-watch": "tsc --watch",
    "build-lint": "tslint -p ./tsconfig.json -c ./tslint.json -t verbose",
    "build-copy-config": "copyfiles -u 2 ./src/data/**/*.json ./dist/data/",
    "build-copy-docs": "copyfiles -u 1 ./src/docs/**/* ./dist",
    "build": "npm-run-all build-clean build-compile build-lint build-copy-docs build-copy-config",
    "serve-mon": "nodemon ./dist/index",
    "start-dev": "run-p serve-mon build-watch"
  },
  "author": "Peter Okwara",
  "license": "MIT",
  "dependencies": {
    "@iota/mam.js": "^1.6.2",
    "crypto": "^1.0.1",
    "fs": "0.0.1-security",
    "ipfs-core": "^0.11.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.4",
    "copyfiles": "^2.4.1",
    "ipfs-core-types": "^0.8.1",
    "nodemon": "^2.0.14",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^4.4.4"
  }
}
